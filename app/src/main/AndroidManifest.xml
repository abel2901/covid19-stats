<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.covid19cases">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_coronavirus_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_coronavirus_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid19cases">
        <activity android:name=".ui.country.CovidCountryDetail"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>